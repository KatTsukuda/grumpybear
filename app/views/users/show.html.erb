<h1>
  <%= @user.org_name%>
  <% if @user == current_user %>
  <%= link_to edit_user_path(@user) do %>
    <span class="glyphicon glyphicon-pencil"></span>
  <% end %>
  <%= link_to "Delete", @user, method: :delete, data: {confirm: "Are you sure?"} %>
<% end %>
</h1>
<p> Email: <%= @user.email %> </p>
<p> Country: <%= @user.country %> </p>
